{
    "name": "stopfen/steam-rest-api-php",
    "description": "A simple PHP REST API project based allyans3/steam-market-api-v2",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "slim/slim": "4.14.0",
        "slim/psr7": "1.7.1",
        "monolog/monolog": "3.9.0",
        "vlucas/phpdotenv": "5.6.0",
        "allyans3/steam-market-api-v2": "4.3.0",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "12.2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}